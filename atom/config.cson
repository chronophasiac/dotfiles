"*":
  Zen:
    showWordCount: "Right"
    softWrap: true
  "atom-beautify":
    general:
      _analyticsUserId: "5719337d-b4d6-4036-8054-c47bf2389086"
      beautifyEntireFileOnSave: false
    ruby:
      rbeautify_path: "/Users/mike/.rbenv/shims/rbeautify"
  "autoclose-html":
    additionalGrammars: [
      "GitHub Markdown"
    ]
  "autocomplete-plus":
    confirmCompletion: "tab"
    fileBlacklist: [
      "*.md"
    ]
    useAlternateScoring: true
  "color-picker":
    preferredFormat: "HSL"
  core:
    themes: [
      "one-dark-ui"
      "monokai"
    ]
  editor:
    fontFamily: "firacode-light"
    invisibles: {}
    showIndentGuide: true
    showInvisibles: true
    softWrapAtPreferredLineLength: true
    undoGroupingInterval: 100
  "exception-reporting":
    userId: "1d229d36-9395-6a4a-f530-54d2a5ef4035"
  "go-debug":
    limitCommandsToGo: true
    panelInitialVisible: false
  "go-plus": {}
  gofmt: {}
  "gometalinter-linter": {}
  linter: {}
  "linter-eslint": {}
  "linter-ruby": {}
  "markdown-writer":
    siteEngine: "jekyll"
  "merge-conflicts":
    gitPath: "/usr/local/bin/git"
  "one-dark-ui":
    layoutMode: "Compact"
  "ruby-test":
    rspecFileCommand: "vagrant ssh -- \"bundle exec bin/rspec --tty {relative_path}\""
    rspecSingleCommand: "vagrant ssh -- \"bundle exec bin/rspec --tty {relative_path}:{line_number}\""
    shell: "/usr/local/bin/zsh"
  "split-diff":
    diffWords: true
    ignoreWhitespace: true
    leftEditorColor: "red"
    rightEditorColor: "green"
    syncHorizontalScroll: true
  tabs:
    enableVcsColoring: true
    usePreviewTabs: true
  "tester-go":
    runCoverageOnSave: true
  "theme-switcher":
    syntaxThemeOne: "monokai"
  "tree-view": {}
  "vim-mode":
    useSmartcaseForSearch: true
  welcome:
    showOnStartup: false
